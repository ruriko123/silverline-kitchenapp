{
  "name": "kitchenapp",
  "version": "1.0.0",
  "description": "silverline kitchen app",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "concurrently  \"npx tsc && tsc-alias\"  \"npx tsc && tsc-alias --watch\" \"nodemon -q dist/index.js\"",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "migration": "typeorm-model-generator -h localhost -p 3306 -d kitchenDB -u root -x password -e mariadb -o .",
    "dbbackup": "typeorm-model-generator -h localhost -p 3306 -d kitchenDB -u root -x password -e mariadb -o ./backup/dbbackup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruriko123/silverline-kitchenapp.git"
  },
  "author": "ruriko123",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ruriko123/silverline-kitchenapp/issues"
  },
  "homepage": "https://github.com/ruriko123/silverline-kitchenapp#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.6.1",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  }
}
